---
title: teste a coisas
layout: dawn/default.njk
permalink: /membros/
---

<div class="site-content">
    <main id="site-main" class="site-main outer" role="main">
        <div class="inner">
            <div class="member-feed site-content">
                {% for member in members %}
                    <article class="member-card ">
                        <a href="/author/{{member.slug}}/" title="{{member.name}}"></a>
                        <div class="author">
                        {{member | log}}
                            <a href="/membros/{{member.slug}}"
                            title="{{member.name}}">
                                <div class="author-image-placeholder u-placeholder square">
                                    {%if member.profile_image[0].url%}
                                        {% image member.profile_image[0].url, member.name, [140 ,200], ["author-image"], ["u-object-fit"] %}
                                    {%endif%}
                                </div>
                            </a>
                            <h1 class="author-name">
                                <a href="/membros/{{member.slug}}/" title="{member.name}"></a>
                                <a
                                href="/membros/{{member.slug}}/" target="_blank"
                                rel="noopener noreferrer">{{member.name}}</a>
                            </h1>
                            <span class="author-meta">{{member.location}}</span>
                            <div class="author-bio">{{member.bio}}
                            </div>
                            <div class="share author-social" style="margin-top:0;"></div>
                        </div>
                    </article>
                {% endfor %}


            </div>
        </div>
    </main>

</div>